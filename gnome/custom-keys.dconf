[/]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom4/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom5/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom6/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom7/']

[custom-keybindings/custom0]
binding='F9'
command='gromit-mpx --toggle'
name='gromit-mpx-wayland-hotkey --toggle'

[custom-keybindings/custom1]
binding='<Shift>F9'
command='gromit-mpx --clear'
name='gromit-mpx-wayland-hotkey --clear'

[custom-keybindings/custom2]
binding='<Ctrl>F9'
command='gromit-mpx --visibility'
name='gromit-mpx-wayland-hotkey --visibility'

[custom-keybindings/custom3]
binding='<Alt>F9'
command='gromit-mpx --quit'
name='gromit-mpx-wayland-hotkey --quit'

[custom-keybindings/custom4]
binding='F8'
command='gromit-mpx --undo'
name='gromit-mpx-wayland-hotkey --undo'

[custom-keybindings/custom5]
binding='<Shift>F8'
command='gromit-mpx --redo'
name='gromit-mpx-wayland-hotkey --redo'

[custom-keybindings/custom6]
command='script --command "QT_QPA_PLATFORM=wayland flameshot gui"'
name='flameshot'

[custom-keybindings/custom7]
binding='<Control>Print'
command='gradia --screenshot=INTERACTIVE'
name='gradia'
